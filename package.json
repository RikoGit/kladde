    "private": true,
    "scripts": {
        "build": "NODE_ENV=production webpack --config configs/webpack.config.js",
        "clean:build": "rm -rf static/build",
        "dev:client": "NODE_ENV=development webpack --watch --config configs/webpack.config.js",
        "dev:server": "NODE_ENV=development node src/server.js",
        "lint": "run-p lint:*",
        "lint:css": "stylelint 'static/**/*.css' 'src/**/*.css'",
        "lint:es": "eslint --ext js,jsx src",
        "prebuild": "npm run clean:build",
        "predev:client": "npm run clean:build",
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "npm run build && node src/server.js",
        "start:dev": "run-p dev:*"
    },
