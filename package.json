        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.7.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-hot-loader": "^4.12.19",
        "react-router-dom": "^5.1.2",
        "style-loader": "^1.1.3",
        "webpack": "^5.0.0-beta.13",
